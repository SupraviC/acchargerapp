<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.supravin.accharger.Authentication">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:visibility="gone"
        android:layout_marginStart="93dp"
        android:layout_marginTop="63dp"
        android:text="Internet Onn" />





    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container"
        android:visibility="visible"
        android:layout_marginLeft="18dp"
        android:layout_height="270dp"
        android:background="@drawable/background_border"
        android:layout_marginTop="22dp"

       >  </RelativeLayout>


    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container10"
        android:visibility="visible"
        android:layout_marginLeft="18dp"
        android:layout_below="@+id/container"
        android:layout_height="270dp"
        android:background="@drawable/background_border"
        android:layout_marginTop="20dp"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Update Settings"
            android:textColor="#000"
            android:textSize="30dp"
            android:id="@+id/textView10"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="2dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_below="@+id/textView10"
            android:layout_height="1dp"
            android:background="#000"
            android:id="@+id/view" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/view"
            android:layout_marginStart="23dp"
            android:textSize="18dp"
            android:layout_marginTop="19dp"
            android:text="Current Build Version" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:textSize="18dp"
            android:layout_alignTop="@+id/textView4"
            android:layout_marginEnd="22dp"
            android:text="1.0" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:layout_alignStart="@+id/textView4"
            android:layout_below="@+id/textView4"
            android:layout_marginTop="37dp"
            android:text="Current Build Name" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:layout_alignEnd="@+id/textView6"
            android:layout_alignTop="@+id/textView7"
            android:text="1.0" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="10dp"
            android:layout_centerHorizontal="true"
            android:text="Check For Update" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container1"
        android:visibility="visible"
        android:layout_marginLeft="335dp"
        android:layout_height="270dp"
       android:layout_alignLeft="@+id/container"
        android:background="@drawable/background_border"
        android:layout_marginTop="22dp"

        >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Power Settings"
            android:textColor="#000"
            android:textSize="30dp"
            android:id="@+id/textView"
          android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="2dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_below="@+id/textView"
            android:layout_height="1dp"
            android:background="#000"/>

        <Button
            android:id="@+id/button4"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="Power Off Device" />

        <Button
            android:id="@+id/button5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/button4"
            android:layout_marginBottom="33dp"
            android:text="Reboot Device" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="22dp"
              android:textSize="18dp"
            android:text="Charger ID" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container110"
        android:visibility="visible"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@+id/container10"
        android:layout_height="270dp"
        android:layout_below="@+id/container1"
        android:background="@drawable/background_border"
        android:layout_marginTop="20dp"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Network Settings"
            android:textColor="#000"
            android:textSize="30dp"
            android:id="@+id/textView110"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="2dp"/>
        <View
            android:layout_below="@+id/textView110"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000"
            android:id="@+id/view2" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView110"
            android:layout_below="@+id/view2"
            android:layout_marginTop="26dp"
            android:textSize="18dp"
            android:text="Device Network" />




        <ImageView
            android:layout_width="4dp"
            android:layout_height="5dp"
            android:src="@drawable/one"
            android:id="@+id/zero"
            android:layout_marginTop="40dp"

            android:layout_marginLeft="250dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />

        <ImageView
            android:layout_width="4dp"
            android:layout_height="10dp"
            android:src="@drawable/one"
            android:id="@+id/one"
            android:layout_toRightOf="@+id/zero"
            android:layout_marginTop="35dp"

            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />

        <ImageView
            android:layout_width="4dp"
            android:layout_height="15dp"
            android:src="@drawable/two"
            android:id="@+id/two"
            android:layout_toRightOf="@+id/one"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />
        <ImageView
            android:layout_width="4dp"
            android:layout_height="20dp"
            android:src="@drawable/three"
            android:id="@+id/three"
            android:layout_toRightOf="@+id/two"
            android:layout_marginTop="25dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />

        <ImageView
            android:layout_width="4dp"
            android:layout_height="25dp"
            android:src="@drawable/four"
            android:id="@+id/four"
            android:layout_toRightOf="@+id/three"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />
        <ImageView
            android:layout_width="4dp"
            android:layout_height="30dp"
            android:src="@drawable/five"
            android:id="@+id/five"
            android:layout_toRightOf="@+id/four"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />

        <ImageView
            android:layout_width="4dp"
            android:layout_height="35dp"
            android:src="@drawable/five"
            android:id="@+id/six"
            android:layout_toRightOf="@+id/five"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/view2"
            tools:ignore="ContentDescription" />




        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView12"
            android:layout_below="@+id/textView12"
            android:layout_marginTop="36dp"
            android:textSize="18dp"
            android:text="Internet Status" />

        <ToggleButton
            android:id="@+id/toggleButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/textView13"
            android:layout_marginEnd="21dp"
            android:text="ToggleButton" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="10dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="10dp"
            android:layout_alignParentLeft="true"
            android:text="Start Internet" />

        <Button
            android:id="@+id/button80"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:text="WifiSettings" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container2"
        android:visibility="visible"
        android:layout_marginLeft="335dp"
        android:layout_height="270dp"
        android:layout_alignLeft="@+id/container1"
        android:background="@drawable/background_border"
        android:layout_marginTop="22dp"

        >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Unit Rate"
            android:textColor="#000"
            android:textSize="30dp"
            android:id="@+id/textView1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="2dp"/>
        <View
            android:layout_below="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:textSize="18dp"
            android:text="Current Rate" />

        <TextView
            android:id="@+id/editText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textView2"
            android:layout_marginTop="10dp"
            android:textSize="18dp"

            android:text="1 Unit = Rs 7.0" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:textSize="18dp"
            android:text="Enter New Rate Here" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:textAlignment="center"
            android:inputType="number"
            android:text="" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_below="@+id/editText2"
            android:layout_centerHorizontal="true"
            android:text="Submit" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="320dp"
        android:id="@+id/container20"
        android:visibility="visible"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/container2"
        android:layout_height="270dp"
        android:layout_toRightOf="@+id/container110"
        android:background="@drawable/background_border"
        android:layout_marginTop="20dp"

        >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Other Settings"
            android:textColor="#000"
            android:textSize="30dp"
            android:id="@+id/textView210"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="2dp"/>
        <View
            android:layout_below="@+id/textView210"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000"
            android:id="@+id/view20" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true"
            android:layout_centerInParent="true"
            android:text="Go Back" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView210"
            android:layout_centerHorizontal="true"
            android:textSize="18dp"
            android:layout_marginTop="20dp"
            android:text="Enter Charger ID" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_below="@+id/textView14"
            android:layout_centerHorizontal="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:hint="CHARGER ID" />



        <Button
            android:id="@+id/button30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText3"

            android:layout_centerInParent="true"
            android:text="SUBMIT" />

    </RelativeLayout>



</RelativeLayout>
